{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node ./src/bin/www.ts",
    "watch:dev": "nodemon ./src/bin/www.ts",
    "start": "node ./dist/src/bin/www.js",
    "dependencies:clean": "rimraf package-lock.json && rimraf node_modules",
    "dependencies:install": "npm i",
    "build": "npm-run-all clean compile postbuildcopy",
    "compile": "npx tsc",
    "postbuildcopy": "npm-run-all copyPublic copyViews",
    "copyPublic": "copyfiles -u 1 \"./src/public/**\" \"./dist/src/\" ",
    "copyViews": "copyfiles -u 1 \"./src/views/**\" \"./dist/src/\" ",
    "clean": "rimraf dist/*",
    "deploy": "git push heroku main"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  }
}
